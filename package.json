{
  "name": "@ruedap/abccss",
  "version": "0.1.0",
  "description": "Easy CSS architecture to arrange in alphabetical order",
  "author": "ruedap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruedap/abccss.git"
  },
  "bugs": {
    "url": "https://github.com/ruedap/abccss/issues"
  },
  "homepage": "https://github.com/ruedap/abccss",
  "keywords": [
    "css",
    "scss",
    "css-architecture",
    "bem",
    "suit-css"
  ],
  "main": "src/index.js",
  "bin": {
    "abccss": "src/index.js"
  },
  "files": [
    "src",
    "*.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "clean:screenshot": "rimraf './__screenshots__'",
    "compile:scss": "webpack --progress --colors",
    "lint:scss": "stylelint 'src/templates/stylesheets/**/*.scss'",
    "lint:scss:fix": "run-s lint:scss -- --fix",
    "format": "run-s format:*",
    "format:js": "prettier --write '**/*.js'",
    "format:scss": "prettier --write 'src/templates/stylesheets/**/*.scss'",
    "sprite:svg": "svgstore -o src/templates/images/sprite.svg -p svg- src/templates/images/sprite_svg/**/*.svg",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "screenshot": "run-s clean:screenshot && storybook-chrome-screenshot -p 6006 -c .storybook",
    "regression": "reg-suit run",
    "test": "run-s compile:scss",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "figlet": "^1.2.0",
    "fs-extra": "^5.0.0",
    "rc": "^1.2.6"
  },
  "devDependencies": {
    "@ruedap/stylelint-config-abccss": "^0.1.0",
    "@storybook/addon-info": "^4.0.0-alpha.6",
    "@storybook/addon-storysource": "^4.0.0-alpha.6",
    "@storybook/addon-viewport": "^4.0.0-alpha.6",
    "@storybook/react": "4.0.0-alpha.6",
    "@storybook/storybook-deployer": "^2.3.0",
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.3",
    "bootstrap": "^4.1.0",
    "conventional-changelog-cli": "^1.3.21",
    "css-loader": "^0.28.11",
    "emma.css": "^0.14.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "reg-keygen-git-hash-plugin": "^0.7.1",
    "reg-notify-github-plugin": "^0.7.2",
    "reg-notify-slack-plugin": "^0.7.1",
    "reg-publish-s3-plugin": "^0.7.3",
    "reg-suit": "^0.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "storybook-chrome-screenshot": "1.0.1",
    "stylelint": "^9.2.0",
    "svgstore-cli": "^1.3.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "peerDependencies": {
    "bootstrap": "^4.1.0",
    "emma.css": "^0.14.0"
  },
  "stylelint": {
    "extends": "@ruedap/stylelint-config-abccss"
  }
}
