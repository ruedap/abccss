{
  "name": "@ruedap/abccss",
  "version": "0.12.0",
  "description": "Easy CSS architecture to arrange in alphabetical order",
  "author": "ruedap",
  "license": "MIT",
  "bin": {
    "abccss": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruedap/abccss.git"
  },
  "bugs": {
    "url": "https://github.com/ruedap/abccss/issues"
  },
  "homepage": "https://github.com/ruedap/abccss",
  "keywords": [
    "css",
    "scss",
    "css-architecture",
    "bem",
    "suit-css"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "abccss",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "types": "lib/index.d.ts",
  "files": [
    "/README.md",
    "/CHANGELOG.md",
    "LICENSE",
    "/bin",
    "/lib",
    "/src",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "scripts": {
    "preversion": "npm test",
    "version": "npm run changelog",
    "release": "npm run oclif:prepack && git add README.md && git commit -m 'docs: Update version within README' && git push && git push --tags",
    "postpublish": "npm run oclif:postpack",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md",
    "compile:scss": "webpack --progress --colors",
    "lint:scss": "stylelint 'src/templates/stylesheets/**/*.scss'",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "format": "run-s format:*",
    "format:js": "prettier --write '**/*.js'",
    "format:scss": "prettier --write 'src/templates/stylesheets/**/*.scss'",
    "format:md": "prettier --write '**/README.md'",
    "sprite:svg": "svgstore -o src/templates/images/sprite.svg -p svg- src/templates/images/sprite_svg/**/*.svg",
    "test": "npm run compile:scss",
    "ci:test": "run-s compile:scss oclif:test reg-suit:screenshot reg-suit:test",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o tmp/storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "reg-suit:clean:screenshot": "rimraf './__screenshots__'",
    "reg-suit:screenshot": "npm run reg-suit:clean:screenshot && storybook-chrome-screenshot -p 6006 -c .storybook",
    "reg-suit:test": "reg-suit run",
    "reg-suit:report": "open .reg/index.html",
    "reg-cli:clean:expected": "rimraf ./.storybook/screenshots/expected && mkdirp ./.storybook/screenshots/expected",
    "reg-cli:clean:actual": "rimraf ./.storybook/screenshots/actual && mkdirp ./.storybook/screenshots/actual",
    "reg-cli:screenshot:expected": "storybook-chrome-screenshot -p 6006 -c .storybook -o ./.storybook/screenshots/expected",
    "reg-cli:screenshot:actual": "storybook-chrome-screenshot -p 6006 -c .storybook -o ./.storybook/screenshots/actual",
    "reg-cli:save:expected": "run-s reg-cli:clean:expected reg-cli:screenshot:expected",
    "reg-cli:save:actual": "run-s reg-cli:clean:actual reg-cli:screenshot:actual",
    "reg-cli:save": "npm run reg-cli:save:expected",
    "reg-cli:test": "run-s reg-cli:save:actual reg-cli:exec",
    "reg-cli:update": "npm run reg-cli:exec -- --update",
    "reg-cli:exec": "reg-cli ./.storybook/screenshots/actual ./.storybook/screenshots/expected ./.storybook/screenshots/diff -R ./.storybook/report.html",
    "reg-cli:report": "open ./.storybook/report.html",
    "oclif:test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "oclif:shrinkwrap": "npm prune --production && rm -f package-lock.json && npm shrinkwrap && git checkout package-lock.json",
    "oclif:prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme  && npm run oclif:shrinkwrap",
    "oclif:postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json && npm install"
  },
  "dependencies": {
    "@oclif/command": "^1.4.33",
    "@oclif/config": "^1.6.31",
    "@oclif/plugin-help": "^2.0.5",
    "change-case": "^3.0.2",
    "fs-extra": "^5.0.0",
    "pkg-dir": "^3.0.0",
    "rc": "^1.2.6",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.33",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@rails/webpacker": "^4.0.0-pre.2",
    "@ruedap/stylelint-config-abccss": "^0.5.0",
    "@storybook/addon-storysource": "^4.0.0-alpha.6",
    "@storybook/addon-viewport": "^4.0.0-alpha.6",
    "@storybook/react": "4.0.0-alpha.6",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.3",
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.3",
    "bootstrap": "^4.3.1",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^1.3.21",
    "css-loader": "^0.28.11",
    "emma.css": "0.15.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "globby": "^8.0.1",
    "jquery": "^3.3.1",
    "mocha": "^5.1.1",
    "node-sass": "^4.8.3",
    "np": "^3.0.4",
    "npm-run-all": "^4.1.2",
    "nyc": "^12.0.2",
    "oclif": "^1.8.5",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.13.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.7.1",
    "reg-notify-slack-plugin": "^0.7.1",
    "reg-publish-s3-plugin": "^0.7.3",
    "reg-suit": "^0.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "sinon": "^6.0.0",
    "storybook-chrome-screenshot": "1.0.1",
    "stylelint": "^9.2.0",
    "svgstore-cli": "^1.3.1",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "peerDependencies": {
    "bootstrap": "^4.1.0",
    "emma.css": "^0.15.0"
  },
  "stylelint": {
    "extends": "@ruedap/stylelint-config-abccss"
  }
}
